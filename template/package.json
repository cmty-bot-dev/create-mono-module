    ]
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@terrajs/mono": "^0.6.0",
    "mono-test-utils": "^1.2.0",
    "ava": "^0.22.0",
    "codecov": "^2.3.1",
    "eslint": "^4.9.0",
    "nyc": "^11.2.1"
  },
  "keywords": [
    "mono",
    "mono-module"
  ]
}
